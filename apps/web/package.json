    "@radix-ui/react-slot": "^1.1.2",
    "better-auth": "^1.3.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.4",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.0.1",
    "vitest": "^3.2.4",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
